<h1>All Invites</h1>
<a href="/invite/new">Create new invite</a>
<table>
  <tr>
    <th>
      Invite Url
    </th>
    <th>
      Guests
    </th>
  </tr>
  {{#invites}}
  <tr>
    <td>{{token}}</td>
    <td>{{guests}}</td>
    <td>
      <form method="POST" action="/invite/{{token}}/delete">
        <input type="submit" value="Delete" />
      </form>
    </td>
  </tr>
  {{/invites}}
</table>
